<configuration>
  <configSections>    
    <section name="applicationConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="serverConfiguration" type="System.Configuration.NameValueSectionHandler" />   
    <section name="commonConfiguration" type="System.Configuration.NameValueSectionHandler" />
    <section name="annotationConfiguration" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <applicationConfiguration>
    <!--Absolute path to GroupDocs license file-->
    <add key="licensePath" value="C:/GroupDocs/Net/Annotation/GroupDocs.Annotation.NET.lic" />
  </applicationConfiguration>
  <serverConfiguration>
    <!--Host where to send API requests-->
    <add key="hostAddress" value="localhost" />
    <!--HTTP Port-->
    <add key="httpPort" value="8080" />
  </serverConfiguration>  
  <commonConfiguration>
     <!--Enable/disable page navigation elements-->
    <add key="isPageSelector" value="true" />   
    <!--Enable/disable document download-->
    <add key="isDownload" value="true" />
    <!--Enable/disable document upload-->
    <add key="isUpload" value="true" />
    <!--Enable/disable document print-->
    <add key="isPrint" value="true" />
    <!--Enable/disable documents browse-->
    <add key="isBrowse" value="true" />   
    <!--Enable/disable rewriting, if false uploaded document with name which exists will be saved as new copy -->
    <add key="isRewrite" value="true" />
  </commonConfiguration>   
  <annotationConfiguration>
    <!--Absolute or relative path to documents storage-->
    <add key="filesDirectory" value="C:/GroupDocs/Documents" />
    <!--Directory where all signed files are stored-->
    <add key="outputDirectory" value="" />
    <!--Default document to view-->
    <add key="defaultDocument" value="" />
    <!--Number of pages for preload, 0 for all-->
    <add key="preloadPageCount" value="0" />
    <!--Directory used to store custom fonts-->
    <add key="fontsDirectory" value="" />
    <!--Enable/disable text annotation-->
    <add key="isTextAnnotation" value="true" />
    <!--Enable/disable area annotation-->
    <add key="isAreaAnnotation" value="true" />
    <!--Enable/disable point annotation-->
    <add key="isPointAnnotation" value="true" />
    <!--Enable/disable text strikeout annotation-->
    <add key="isTextStrikeoutAnnotation" value="true" />
    <!--Enable/disable polyline annotation-->
    <add key="isPolylineAnnotation" value="true" />
    <!--Enable/disable text field annotation-->
    <add key="isTextFieldAnnotation" value="true" />    
     <!--Enable/disable watermark annotation-->
    <add key="isWatermarkAnnotation" value="true" />
     <!--Enable/disable text replacement annotation-->
    <add key="isTextReplacementAnnotation" value="true" />
     <!--Enable/disable arrow annotation-->
    <add key="isArrowAnnotation" value="true" />
     <!--Enable/disable text redaction annotation-->
    <add key="isTextRedactionAnnotation" value="true" />
   <!--Enable/disable resource redaction annotation-->
    <add key="isResourcesRedactionAnnotation" value="true" />
      <!--Enable/disable text underline annotation-->
    <add key="isTextUnderlineAnnotation" value="true" />
   <!--Enable/disable distance annotation-->
    <add key="isDistanceAnnotation" value="true" />    
    <!--Enable/disable download original document-->
    <add key="isDownloadOriginal" value="true" />
    <!--Enable/disable download annotated document-->
    <add key="isDownloadAnnotated" value="true" />
  </annotationConfiguration>
  <appSettings>
    <add key="vs:EnableBrowserLink" value="false" />
    <add key="vs:EnableBrowserLink" value="false" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" maxRequestLength="404857" executionTimeout="3600" />   
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true"></modules>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="414573568" />
      </requestFiltering>
    </security>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
